  "homepage": "https://github.com/cos730-2018-implementation-users/users-submodule",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cos730-2018-implementation-users/users-submodule"
  },
